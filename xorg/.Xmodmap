clear shift
clear lock
clear control
clear mod1
clear mod2
clear mod3
clear mod4
clear mod5

keycode 22 = backslash bar
keycode 23 = BackSpace
keycode 51 = Tab

keycode 66 = Control_L
keycode 50 = Shift_L
keycode 37 = Hyper_L
keycode 133 = Super_L
keycode 64 = Alt_L
keycode 108 = Alt_R
keycode 134 = Super_R
keycode 135 = Multi_key
keycode 105 = Hyper_R
keycode 62 = Shift_R

add shift = Shift_L
add shift = Shift_R
add control = Control_L
add mod1 = Alt_L
add mod1 = Alt_R
add mod2 = Num_Lock
add mod3 = Super_L
add mod3 = Super_R
add mod4 = Hyper_L
add mod4 = Hyper_R
add mod5 = Multi_key
